<?xml version="1.0"?>
<extension key="nbrportalstuff" type="module">
  <file>nbrportalstuff</file>
  <name>NIHR BioResource portal stuff</name>
  <description>NIHR BioResource specific extension for stuff to do with the portal</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Erik Hommel</author>
    <email>erik.hommel@civicoop.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/partners/civicoop/nihr-bioresearch/nbrportalstuff</url>
    <url desc="Documentation">https://lab.civicrm.org/partners/civicoop/nihr-bioresearch/nbrportalstuff</url>
    <url desc="Support">https://civicoop.org</url>
